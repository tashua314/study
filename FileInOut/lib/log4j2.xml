<?xml version="1.0" encoding="UTF-8" ?>
<configuration>
  <appenders>
    <Console name="console" target="SYSTEM_OUT">
      <PatternLayout pattern="%-5level [%t]: %msg%n" />
    </Console>
  </appenders>
  
  <loggers>
    <root level="debug">
      <appender-ref ref="console" />
    </root>
  </loggers>
</configuration>